<% @users.each do |u| %>
  <p>
    <span><%= u.first_name %></span>
    <span><%= u.last_name %></span>
    <span><%= u.email %></span>
    <span class="link_button edit_button"><%= link_to "Edit", users_path(:params => params) %></span>
    <span class="link_button delete_button"><%= link_to "Delete", users_path(:params => params) %></span>
  </p>
<% end %>
<%= button_to "New User" %>
